  "compilerOptions": {
    "target": "ESNext",

    "module": "CommonJS",

    "moduleResolution": "node",

    "outDir": ".",

    "esModuleInterop": true,

    "forceConsistentCasingInFileNames": true,

    "strict": true,

    "skipLibCheck": true
  },

  "include": [
    "../src/bin",
    "../src/constants/error.ts",
    "../src/constants/process.ts",
    "../src/utils/process.ts",
    "../src/utils/gatsby.ts"
  ]
}
